#: ../../PacketsStop.cs:47
msgid "[数据包拦截]重新加载配置完毕。"
msgstr "[PacketStop] Reload configuration completed."

#: ../../Commands.cs:21
msgid "/pksp add 玩家名字 ——将指定玩家添加到拦截名单\n"
msgstr "/pksp add <player name> —— add the specified player to the intercept list\n"

#: ../../Commands.cs:22
msgid "/pksp del 玩家名字 ——将指定玩家从拦截名单移除\n"
msgstr "/pksp del <player name> —— removes the specified player from the intercept list\n"

#: ../../Commands.cs:20
msgid "/pksp list ——列出拦截名单\n"
msgstr "/pksp list —— List of interceptors\n"

#: ../../Commands.cs:19
msgid "/pksp off ——关闭数据包拦截\n"
msgstr "/pksp off —— Turn off packet interception\n"

#: ../../Commands.cs:18
msgid "/pksp on ——开启数据包拦截\n"
msgstr "/pksp on —— Turn on packet interception\n"

#: ../../Commands.cs:23
msgid "/pksp reset ——清空拦截名单"
msgstr "/pksp reset —— Clear the intercept list"

#: ../../Commands.cs:95
#, csharp-format
msgid "成功从拦截名单删除玩家: {0} "
msgstr "Successfully deleted player from the intercepted list: {0} "

